{
  "name": "ayoub-amzil/failed-job-notifier",
  "description": "A Laravel package to send email notifications on failed queued jobs.",
  "type": "library",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Ayoub Amzil",
      "email": "dev.amzil.ayoub@example.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "AyoubAmzil\\FailedJobNotifier\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AyoubAmzil\\FailedJobNotifier\\FailedJobServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
